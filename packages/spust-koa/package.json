{
  "name": "spust-koa",
  "description": "Write your Spust Koa backend server using JSX and React.",
  "version": "0.3.1",
  "license": "MIT",
  "keywords": [
    "spust",
    "universal",
    "koa",
    "jsx",
    "jsx-to-koa",
    "serve"
  ],
  "main": "lib/index.js",
  "repository": "https://github.com/michalkvasnicak/spust/tree/master/packages/spust-koa",
  "author": "Michal Kvasničák",
  "dependencies": {
    "es6-error": "^4.0.2",
    "helmet": "^3.8.1",
    "hpp": "^0.2.2",
    "kcors": "^2.2.1",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-compose": "^4.0.0",
    "koa-connect": "^2.0.0",
    "koa-session-minimal": "^3.0.4",
    "koa-static-cache": "^5.1.1",
    "spust": "^0.18.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "flow-bin": "^0.52.0",
    "flow-copy-source": "^1.2.0",
    "graphql": "^0.10.5",
    "graphql-server-koa": "^1.0.5",
    "graphql-tools": "^1.1.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "rimraf": "^2.6.1",
    "supertest": "^3.0.0"
  },
  "peerDependencies": {
    "graphql-server-koa": "^0.9.0 || ^1.0.0",
    "prop-types": ">=15.0.0 <16.0.0",
    "react": ">=15.0.0 <16.0.0",
    "react-dom": ">=15.0.0 <16.0.0"
  },
  "scripts": {
    "prebuild": "rimraf lib/*",
    "build:lib": "babel src --ignore \"**/__tests__/*.js\" --out-dir lib",
    "build:flow": "flow-copy-source -v -i \"**/__tests__/*.js\" src lib",
    "build": "npm run build:lib && npm run build:flow"
  }
}
